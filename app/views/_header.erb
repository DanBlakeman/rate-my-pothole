<header class="header">
    <a href='/'>
      <img class="logo" src="/images/logo.png">
    </a>

  <div class="session-buttons">
    <% if current_user %>
      <form  action="/sessions/end" method="get" id="sign_out" class="sign_out">
        <button>Sign out</button>
      </form>
    <% else %>
      <form  action="/sessions/new" method="get" id="sign_in" class="sign_in">
        <button>Sign in</button>
      </form>
      <form action="/users/new">
        <button>Sign up</button>
      </form>
    <% end %>
</header>