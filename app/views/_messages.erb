<% if flash[:errors] %>
  <div class="errors">
    <ul>
      <% flash[:errors].each do |error| %>
        <li class="error_text"><%= error %></li>
      <% end %>
    </ul>
    </div>
<% end %>


<div class="welcome">
  <% if current_user %>
    <div>Welcome, <%= current_user.username %>!</div>
  <% end %>
</div>